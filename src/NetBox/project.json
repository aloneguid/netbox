{
   "version": "1.0.0-alpha-*",

   "authors": [ "Ivan Gavryliuk" ],

   "title": "netbox",

   "copyright": "Copyright (c) 2015-2016 by Ivan Gavryliuk",

   "packOptions": {

      "iconUrl": "https://alonestore.blob.core.windows.net/nuget/support.png",

      "projectUrl": "https://github.com/aloneguid/support",

      "summary": "Useful extra functions missing or not easily accessible in .NET framework."
   },

   "buildOptions": {
      "embed": [
         "Data/first-names.txt",
         "Data/last-names.txt"
      ],
      "keyFile": "../support.snk"
   },

   "frameworks": {
      "netstandard1.6": {
         "imports": "dnxcore50",
         "dependencies": {
            "NETStandard.Library": "1.6.0",
            "Newtonsoft.Json": "9.0.1"
         },
         "buildOptions": {
            "define": [ "NETSTANDARD" ]
         }
      },
      "net45": {
         "dependencies": {
            "Newtonsoft.Json": "8.0.3"
         },
         "buildOptions": {
            "define": [ "NETFULL" ]
         },
         "frameworkAssemblies": {
            "System.Xml": "4.0.0.0",
            "System.Web": "4.0.0.0"
         }
      }
   }
}
