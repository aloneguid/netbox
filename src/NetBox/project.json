         "imports": "dnxcore50",
         "dependencies": {
            "NETStandard.Library": "1.6.0",
            "Newtonsoft.Json": "9.0.1" 
         },
         "buildOptions": {
            "define": [ "NETSTANDARD" ]
         }
      },
      "net45": {
         "dependencies": {
            "Newtonsoft.Json": "8.0.3"
         },
         "buildOptions": {
            "define": [ "NETFULL" ]
         },
         "frameworkAssemblies": {
            "System.Xml": "4.0.0.0",
            "System.Web": "4.0.0.0"
         }
      }
   }
}
